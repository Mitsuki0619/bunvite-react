{
  "name": "bunvite-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:js": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:css": "stylelint '**/*.css'",
    "lint:html": "markuplint '**/*.tsx'",
    "fmt": "prettier ./src --write",
    "lint:fix": "eslint . --fix --ext ts,tsx",
    "lint:all": "bun run lint:js & bun run lint:html & bun run lint:css",
    "preview": "vite preview"
  },
  "dependencies": {
    "install": "^0.13.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "^3.10.0",
    "@markuplint/react-spec": "^3.11.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "bun-types": "latest",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "markuplint": "^3.13.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^11.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "module": "index.ts"
}